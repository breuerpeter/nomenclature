% Encoding: UTF-8

# General

@symbol{vecState,
    name = {\ensuremath{\bm{x}}},
    description = {state vector},
    identifier = {general}
}

# IMU

@symbol{vecGyroBias,
    name = {\ensuremath{\bm{b}^G}},
    description = {gyroscope (ang. vel.) bias vector},
    identifier = {imu}
}

@symbol{vecAccelBias,
    name = {\ensuremath{\bm{b}^A}},
    description = {accelerometer (lin. acc.) bias vector},
    identifier = {imu}
}

@symbol{vecGyroNoise,
    name = {\ensuremath{\bm{n}^G}},
    description = {gyroscope (ang. vel.) additive zero-mean Gaussian white noise vector},
    identifier = {imu}
}

@symbol{vecAccelNoise,
    name = {\ensuremath{\bm{n}^A}},
    description = {accelerometer (lin. acc.) additive zero-mean Gaussian white noise vector},
    identifier = {imu}
}